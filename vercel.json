{
  "version": 2,
  "name": "tees-by-shelsea",
  "builds": [
    {
      "src": "simple-server.js",
      "use": "@vercel/node"
    },
    {
      "src": "public/**",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "simple-server.js"
    },
    {
      "src": "/(.*)",
      "dest": "public/$1"
    }
  ]
}